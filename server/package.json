{
  "name": "telefonbuch-server",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch --env-file=.env.local src/index.ts",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "dependencies": {
    "@apollo/server": "^5.4.0",
    "bcryptjs": "^3.0.3",
    "graphql": "^16.12.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^7.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.3.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  }
}
